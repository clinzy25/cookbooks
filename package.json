{
  "name": "cookbooks",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "install": "concurrently \"pnpm i --prefix client\" \"pnpm i --prefix server\"",
    "dev": "NODE_ENV=development concurrently \"npm run dev --prefix server\" \"npm run dev --prefix client\"",
    "build": "concurrently \"npm run build --prefix server\" \"npm run build --prefix client\"",
    "stage": "NODE_ENV=staging concurrently \"npm start --prefix server\" \"npm start --prefix client\"",
    "start": "NODE_ENV=production concurrently \"npm start --prefix server\" \"npm start --prefix client\"",
    "db:create": "knex migrate:latest --knexfile server/src/db/knexfile.js",
    "db:seed": "knex seed:run --knexfile server/src/db/knexfile.js",
    "db:destroy": "knex migrate:down --knexfile server/src/db/knexfile.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.3.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "concurrently": "^8.0.1",
    "eslint": "^8.37.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "babel-preset-next": "^1.4.0",
    "sharp": "^0.32.1",
    "uuid": "^9.0.0"
  }
}
